        "backgroundColor": "#FBEAEB",
        "primaryColor": "#4BA3C3",
        "secondaryColor": "#000000",
        "successColor": "#320707",
        "infoColor": "#0BAFD0",
        "warningColor": "#DBA400",
        "dangerColor": "#ffeb69",
        "mainFont": "Golos Text",
        "display1Font": "Golos Text",
        "display1Size": 5,
        "display2Font": "Golos Text",
        "display2Size": 4,
        "display5Font": "Golos Text",
